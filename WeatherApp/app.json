{
  "expo": {
    "name": "WeatherApp",
    "slug": "WeatherApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "light",
    "privacy": "public",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to take photos",
        "NSLocationWhenInUseUsageDescription": "We need access to your location for location-based features",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload images"
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.WeatherApp"
      // "config": {
      //   "keystoreAlias": "Bashir",
      //   "keystorePath": "./your-keystore-file.keystore",
      //   "keystorePassword": "incorrect",
      //   "keyPassword": "incorrect"
      // }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c856d3e5-a168-4ed5-a2c9-23504ff1a58c"
      }
    }
  }
}
